<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <submitter>cube84user@wcb.com</submitter>
        <type>user</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Pre_Organization_s_Legal_Name__c</field>
        <field>Fiscal_Year_End__c</field>
        <field>Pre_value_Fiscal_Year_End__c</field>
        <field>FEIN__c</field>
        <field>Pre_Value_FEIN__c</field>
        <field>Owner</field>
        <field>Approved_By__c</field>
        <field>Rejected_by__c</field>
        <field>Approved_Date__c</field>
        <field>Rejected_date__c</field>
        <field>Record_Owner__c</field>
        <field>Rejected_FIEN__c</field>
        <field>Rejected_Fiscal_Year_End__c</field>
        <field>Rejected_Organization_s_Legal_Name__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Approval_request_approved</name>
                <type>Alert</type>
            </action>
            <action>
                <name>approval_request_approved_for_customer</name>
                <type>Alert</type>
            </action>
            <action>
                <name>Approved</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>update_FEIN</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Update_Old_Fiscal_Year_End</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>update_Old_Org_Legal_Name</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Owner</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Step 1</label>
        <name>Step_1</name>
        <rejectionActions>
            <action>
                <name>Approval_request_rejected</name>
                <type>Alert</type>
            </action>
            <action>
                <name>Populate_FEIN_from_Pre_FEIN</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Populate_Fiscal_year_from_Pre_value_F</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>populate_Pre_Org_Legal_name</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Rejected</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>approval_request_approved_last_mod</name>
            <type>Alert</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>approval_request_rejected_last_mod</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Record_change_request_Approval_Notification</name>
            <type>Alert</type>
        </action>
        <action>
            <name>update_approval_process_to_pending</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Account record changes Approval2</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
